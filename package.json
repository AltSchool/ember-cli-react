{
  "name": "ember-cli-react",
  "version": "0.3.0",
  "description": "Use React component hierarchies in your Ember app.",
  "keywords": ["ember-addon"],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": ["prettier --write", "eslint --fix", "git add"],
    "+(*.{json,css}|.prettierrc|.watchmanconfig)": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "https://github.com/AltSchool/ember-cli-react",
  "engines": {
    "node": ">= 4.0.0"
  },
  "author": "alex@altschool.com",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-ajax": "^3.0.0",
    "ember-browserify": "^1.1.7",
    "ember-cli": "2.13.1",
    "ember-cli-chai": "^0.4.1",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-eslint": "^3.0.0",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-htmlbars-inline-precompile": "^0.4.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "^0.14.3",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.0.0",
    "ember-sinon": "0.5.0",
    "ember-source": "~2.13.0",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "loader.js": "^4.2.3",
    "prettier": "^1.9.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "dependencies": {
    "bower": "^1.8.2",
    "broccoli-react": "^0.8.0",
    "ember-cli-babel": "^6.0.0",
    "rsvp": "^3.2.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">=1.13.0 <=2.13.0"
    }
  }
}
