{
  "name": "ember-cli-react",
  "version": "1.0.2",
  "description": "Use React component hierarchies in your Ember app.",
  "keywords": [
    "ember-addon"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "+(*.{json,css,md}|.prettierrc|.watchmanconfig)": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "https://github.com/AltSchool/ember-cli-react",
  "engines": {
    "node": ">= 4.0.0"
  },
  "author": "alex@altschool.com",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~2.15.1",
    "ember-cli-chai": "^0.4.1",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.0.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "^0.14.3",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.0.0",
    "ember-sinon": "0.5.0",
    "ember-source": "~2.15.0",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "loader.js": "^4.2.3",
    "prettier": "^1.9.2"
  },
  "dependencies": {
    "broccoli-react": "^0.8.0",
    "ember-auto-import": "^1.0.1",
    "ember-cli-babel": "^6.3.0",
    "react": "^15.5.4 || ^16.0.0",
    "react-dom": "^15.5.4 || ^16.0.0",
    "semver": "^6.0.0"
  },
  "peerDependencies": {
    "react": "^15.5.4 || ^16.0.0",
    "react-dom": "^15.5.4 || ^16.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
